<VBox id="vbox" prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="javafx.LoginToAppController">
    <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
      <children>
        <Label alignment="CENTER" layoutX="155.0" layoutY="177.0" style="&#10;" text="" textAlignment="CENTER" textFill="#9f9f9f" wrapText="false">
          <font>
            <Font size="18.0" />
          </font>
        </Label>
            <Button fx:id="loginButton"         layoutX="32.0" layoutY="267.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="149.0" text="Login"              id="loginButtons" onAction="#tryLogon" />
            <Button fx:id="createNewAccount"    layoutX="32.0" layoutY="390.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="185.0" text="Create New Account" id="bevel-grey" onAction="#registermyaccount" />
            <ImageView fitHeight="600.0" fitWidth="450.0" layoutX="304.0" layoutY="105.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="/images/1523877550-03.png" />
               </image>
            </ImageView>
          <ImageView fitHeight="600.0" fitWidth="450.0" layoutX="200.0" layoutY="20.0" pickOnBounds="true" preserveRatio="true">
              <image>
                  <Image url="/images/wodi.JPG" />
              </image>
            </ImageView>
            <Label layoutX="32.0" layoutY="115.0" text="Email Address" textOverrun="LEADING_ELLIPSIS" id="texts"/>
            <Label layoutX="32.0" layoutY="182.0" text="Password" id="texts" />
            <TextField  fx:id="userName" layoutX="32.0" layoutY="132.0" />
            <PasswordField   fx:id="password" layoutX="32.0" layoutY="200.0" />
      </children>
    </AnchorPane>
  </children>
</VBox>
